{
  "name": "billintel-web",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/billintel-web/src",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/billintel-web",
        "configFile": "apps/billintel-web/vite.config.ts",
        "root": "apps/billintel-web"
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "options": {
        "buildTarget": "billintel-web:build",
        "configFile": "apps/billintel-web/vite.config.ts",
        "root": "apps/billintel-web"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
