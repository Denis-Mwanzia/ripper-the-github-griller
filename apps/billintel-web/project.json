{
  "name": "billintel-web",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/billintel-web/src",
  "targets": {
    "build": {
      "executor": "@nx/web:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/billintel-web",
        "index": "apps/billintel-web/index.html",
        "baseHref": "/",
        "tsConfig": "apps/billintel-web/tsconfig.json",
        "assets": ["apps/billintel-web/public"],
        "styles": ["apps/billintel-web/src/index.css"]
      }
    },
    "serve": {
      "executor": "@nx/web:dev-server",
      "options": {
        "buildTarget": "billintel-web:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
